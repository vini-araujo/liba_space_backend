LIVEKIT_URL=wss://<project>.livekit.cloud
LIVEKIT_API_KEY=<your_livekit_api_key>
LIVEKIT_API_SECRET=<your_livekit_api_secret>

AGENT_NAME=livekit-agent

# Low-latency TTS model (provider/model). Examples:
# cartesia/sonic-turbo, elevenlabs/eleven_flash_v2_5, rime/mistv2
TTS_MODEL=cartesia/sonic-turbo

TAVUS_API_KEY=<your_tavus_api_key>
TAVUS_REPLICA_ID=<your_tavus_replica_id>
TAVUS_PERSONA_ID=<your_tavus_persona_id>
